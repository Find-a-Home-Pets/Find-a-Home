{
  "expo": {
    "scheme": "exp+find-a-home-app",
    "name": "FindAHome",
    "slug": "FindAHome",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#38202E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.pablo99nunez.findAHome"
    },
    "plugins": [
      [
        "./plugins/withAndroidMainActivityAttributes",
        {
          "android:showWhenLocked": true,
          "android:turnScreenOn": true
        }
      ],
      ["@react-native-google-signin/google-signin"],
      [
        "expo-notifications",
        {
          "notification.icon": ["./src/images/bg1-welcome.png"]
        }
      ]
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.pablo99nunez.findAHome",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAZbpPU8syf-oumM0hRb_VeqRxiGfeoaGQ"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "28a328de-def4-4bdf-8c6c-d3771fe0a0f2"
      }
    },
    "owner": "pablo99nunez"
  }
}
